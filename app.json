{
  "name": "main",
  "expo": {
    "name": "女孩踢球",
    "slug": "GirlsFootballApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "girlsfootball",
    "userInterfaceStyle": "automatic",
   
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.girlsfootball.app",
      "associatedDomains": [
        "applinks:sfgcrobnxslmdhnzwhhm.supabase.co"
      ],
      "jsEngine": "hermes",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.girlsfootball.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "girlsfootball",
              "host": "*",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "jsEngine": "hermes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4bfd4271-4332-4362-8a36-ffdc6339aac8"
      }
    },
    "owner": "addressee3"
    
  }
}
